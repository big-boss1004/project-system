<Project Sdk="Microsoft.NET.Sdk">

  <!--
    The purpose of this project is not for building an assembly. It is for building the Microsoft.VisualStudio.ProjectSystem.Managed project as the files from that are a dependency for the .swixproj.
    Additionally, it ends up running the Nerdbank.GitVersioning mechanism via PackageRestore. This version number is then passed to the .swixproj for as the VSIX version.
  -->

  <Import Project="..\..\eng\imports\VisualStudio.props" />

  <ItemGroup>
    <!-- <ProjectReference Include="SetupManifest.vsmanproj" ReferenceOutputAssembly="false" SetConfiguration="Configuration=$(Configuration)" Targets="@(Targets)" /> -->
    <ProjectReference Include="SetupManifest.vsmanproj" ReferenceOutputAssembly="false" Targets="Restore;Build" />
  </ItemGroup>

  <Target Name="FixGlobalPropertiesToRemove" BeforeTargets="ResolveProjectReferences">
    <ItemGroup>
      <_MSBuildProjectReferenceExistent Update="@(_MSBuildProjectReferenceExistent)" RemoveMetadata="GlobalPropertiesToRemove" />
    </ItemGroup>
  </Target>

  <!-- Builds the associated .vsmanproj to produce a .vsman file for VS insertions. -->
  <!-- <Target Name="VSManifestBuild" AfterTargets="Build">
    <MSBuild Projects="$(MSBuildThisFileDirectory)SetupManifest.vsmanproj" Targets="Restore;Build" />
  </Target> -->

  <!-- <Target Name="VSManifestRestore" AfterTargets="Restore">
    <MSBuild Projects="$(MSBuildThisFileDirectory)SetupManifest.vsmanproj" Targets="Restore" />
  </Target> -->

</Project>