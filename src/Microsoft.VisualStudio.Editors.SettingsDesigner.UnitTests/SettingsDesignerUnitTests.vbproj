    <UsingPartitionWizard>true</UsingPartitionWizard>
    <UsingProjectWizardMicrosoftVSDesigner>true</UsingProjectWizardMicrosoftVSDesigner>
  </PropertyGroup>
  <PropertyGroup>
    <FileUpgradeFlags>
    </FileUpgradeFlags>
    <UpgradeBackupLocation>
    </UpgradeBackupLocation>
  </PropertyGroup>
  <PropertyGroup>
    <DocumentationFile>SettingsDesignerUnitTests.xml</DocumentationFile>
    <BinplaceSymbols>false</BinplaceSymbols>
    <AssemblyAttributeComVisible>true</AssemblyAttributeComVisible>
    <GenerateAssemblyRefs>true</GenerateAssemblyRefs>
    <AssemblyAttributeClsCompliant>false</AssemblyAttributeClsCompliant>
    <NoStdLib>false</NoStdLib>
    <NoExplicitReferenceToStdLib>true</NoExplicitReferenceToStdLib>
    <RootNamespace></RootNamespace>
    <OptionCompare>binary</OptionCompare>
    <OptionExplicit>On</OptionExplicit>
    <OptionStrict>On</OptionStrict>
    <TargetFramework>net46</TargetFramework>
  </PropertyGroup>

  <!-- 
