    <UsingProjectVSCommonMsVsOleInterop>true</UsingProjectVSCommonMsVsOleInterop>
    <UsingProjectVSCommonMsVsShellInterop>true</UsingProjectVSCommonMsVsShellInterop>
    <UsingProjectVSCommonMsVsShellInterop100>true</UsingProjectVSCommonMsVsShellInterop100>
    <UsingProjectVSCommonMsVsTextManagerInterop>true</UsingProjectVSCommonMsVsTextManagerInterop>
    <UsingProjectVSCommonMsVsVSHelp>true</UsingProjectVSCommonMsVsVSHelp>
    <UsingProjectVSCommonVslangproj>true</UsingProjectVSCommonVslangproj>
    <UsingProjectVSCommonVslangproj2>true</UsingProjectVSCommonVslangproj2>
    <UsingProjectVSCommonVslangproj80>true</UsingProjectVSCommonVslangproj80>
    <UsingPartitionVsdata>true</UsingPartitionVsdata>
