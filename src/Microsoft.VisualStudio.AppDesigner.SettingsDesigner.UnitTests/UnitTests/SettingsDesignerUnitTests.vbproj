  <PropertyGroup>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <DocumentationFile>SettingsDesignerUnitTests.xml</DocumentationFile>
    <CreateTlb>true</CreateTlb>
    <UseVsVersion>true</UseVsVersion>
    <EnableDefaultItems>false</EnableDefaultItems> <!-- Excludes all the test files -->
    <AssemblyAttributeComVisible>true</AssemblyAttributeComVisible>
    <GenerateAssemblyRefs>true</GenerateAssemblyRefs>
    <GenerateVrgFromRegAsmOutput>true</GenerateVrgFromRegAsmOutput>
    <GenerateVrgReferencePaths>$(SdkRefPath)</GenerateVrgReferencePaths>
    <GenerateSkuVrg>false</GenerateSkuVrg>
    <NoRuntimeCompatibilityAttribute>true</NoRuntimeCompatibilityAttribute>
    <RootNamespace></RootNamespace>
    <TargetFramework>net46</TargetFramework>
  </PropertyGroup>

